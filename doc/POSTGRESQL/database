(
	ecs_id serial,
	ecs_descricao varchar(35)
);
ALTER TABLE estados_civis
	ALTER COLUMN ecs_id SET NOT NULL,
	ALTER COLUMN ecs_descricao SET NOT NULL,
	ADD PRIMARY KEY(ecs_id)


