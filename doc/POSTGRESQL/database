	ALTER COLUMN cms_descricao SET NOT NULL,
	ALTER COLUMN cms_codigo_operadora SET NOT NULL,
	ALTER COLUMN cms_codigo_registro_ans SET NOT NULL,
	ADD PRIMARY KEY(cms_id);
