delete from pessoas_fisicas where psf_id = 1
select * from pessoas_fisicas
select * from medicos_list_view


drop view medicos_List_View





create view medicos_list_view as select 
	med_id,
	psf.psf_nome,
	psf.psf_nome_mae,
	psf_estado_civil_id,
	med_numero_crm,
	med_crm_uf_id,
	efs.efs_sigla,
	med_cnes,
	psf_cpf,
	psf_rg,
	psf_sexo,
	psf_data_nascimento
	from medicos med 
	inner join pessoas pss on med.med_pss_id = pss.pss_id
	left join pessoas_fisicas psf on psf.psf_pss_id = pss.pss_id
	left join estados_federativos efs on efs.efs_id = med.med_crm_uf_id;



select
	med_id,
	psf.psf_nome,
	psf.psf_nome_mae,
	psf_estado_civil_id,
	med_numero_crm,
	med_crm_uf_id,
	efs.efs_sigla,
	med_cnes,
	psf_cpf,
	psf_rg,
	psf_sexo,
	to_char(psf_data_nascimento,'DD/MM/YYYY'),
	from medicos med 
	inner join pessoas pss on med.med_pss_id = pss.pss_id
	left join pessoas_fisicas psf on psf.psf_pss_id = pss.pss_id
	left join estados_federativos efs on efs.efs_id = med.med_crm_uf_id;


